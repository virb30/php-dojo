{
    "phpunit.relativeFilePath": true,
    "phpunit.files": "{test,tests,Test,Tests}/**/*Test.php",
    "phpunit.phpunit": "/var/www/html/vendor/bin/phpunit",
    "phpunit.php": "/usr/bin/docker-compose exec -T --user ${UID}:${GID} php php",
